{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "id",
    "label",
    "kind"
  ],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-z0-9]+(-[a-z0-9]+)*$"
    },
    "label": {
      "type": "string",
      "minLength": 1
    },
    "kind": {
      "enum": [
        "luminary",
        "planet",
        "asteroid",
        "point",
        "hypothetical"
      ]
    },
    "symbol": {
      "type": "string"
    },
    "glyph_codepoint": {
      "type": "string"
    },
    "mean_orb_deg": {
      "type": "number"
    },
    "avg_speed_deg_per_day": {
      "type": "number"
    },
    "retrograde_cycles": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "color_id": {
      "type": "string",
      "pattern": "^[a-z0-9]+(-[a-z0-9]+)*$"
    },
    "keywords": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "source_ids": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^[a-z0-9]+(-[a-z0-9]+)*$"
      }
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^[a-z0-9]+(-[a-z0-9]+)*$"
      }
    }
  },
  "additionalProperties": false
}