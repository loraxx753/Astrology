{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "id",
    "label",
    "symbol",
    "element_id",
    "modality_id",
    "polarity",
    "ecliptic_start",
    "ecliptic_end"
  ],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-z0-9]+(-[a-z0-9]+)*$"
    },
    "label": {
      "type": "string",
      "minLength": 1
    },
    "symbol": {
      "type": "string",
      "minLength": 1
    },
    "element_id": {
      "enum": [
        "fire",
        "earth",
        "air",
        "water"
      ]
    },
    "modality_id": {
      "enum": [
        "cardinal",
        "fixed",
        "mutable"
      ]
    },
    "polarity": {
      "enum": [
        "yin",
        "yang"
      ]
    },
    "ecliptic_start": {
      "type": "number"
    },
    "ecliptic_end": {
      "type": "number"
    },
    "default_color_id": {
      "type": "string",
      "pattern": "^[a-z0-9]+(-[a-z0-9]+)*$"
    },
    "keywords": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "body_parts": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "season_hint": {
      "enum": [
        "spring",
        "summer",
        "autumn",
        "winter"
      ]
    },
    "glyph_codepoint": {
      "type": "string"
    },
    "domicile_candidates": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^[a-z0-9]+(-[a-z0-9]+)*$"
      }
    },
    "exaltation_candidates": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^[a-z0-9]+(-[a-z0-9]+)*$"
      }
    },
    "detriment_candidates": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^[a-z0-9]+(-[a-z0-9]+)*$"
      }
    },
    "fall_candidates": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^[a-z0-9]+(-[a-z0-9]+)*$"
      }
    },
    "source_ids": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^[a-z0-9]+(-[a-z0-9]+)*$"
      }
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^[a-z0-9]+(-[a-z0-9]+)*$"
      }
    }
  },
  "additionalProperties": false
}